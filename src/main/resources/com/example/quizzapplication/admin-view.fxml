<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ButtonBar?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Tab?>
<?import javafx.scene.control.TabPane?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<AnchorPane fx:id="rootPane" prefHeight="860.0" prefWidth="1559.0" style="-fx-background-color: #FFA88F;" xmlns="http://javafx.com/javafx/25" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.quizzapplication.miscControllers.AdminController">
    <TabPane tabClosingPolicy="UNAVAILABLE" AnchorPane.bottomAnchor="0" AnchorPane.leftAnchor="0" AnchorPane.rightAnchor="0" AnchorPane.topAnchor="0">
        <tabs>
            <Tab text="Manage Users">
                <content>
                    <AnchorPane>
                        <VBox spacing="10" AnchorPane.bottomAnchor="10" AnchorPane.leftAnchor="10" AnchorPane.rightAnchor="10" AnchorPane.topAnchor="10">
                            <children>
                                <Label style="-fx-font-size: 32; -fx-alignment: center;" text="Manage Students">
                                    <VBox.margin>
                                        <Insets bottom="10" />
                                    </VBox.margin>
                                </Label>

                                <ButtonBar>
                                    <buttons>
                                        <Button alignment="CENTER" contentDisplay="CENTER" mnemonicParsing="false" style="-fx-background-color: #56D158;" text="Add User" textFill="WHITE" />
                                        <Button alignment="CENTER" contentDisplay="CENTER" mnemonicParsing="false" style="-fx-background-color: #7353DB;" text="Edit User" textFill="WHITE" />
                                        <Button alignment="CENTER" contentDisplay="CENTER" mnemonicParsing="false" style="-fx-background-color: #DB6353;" text="Delete User" textFill="WHITE" />
                                    </buttons>
                                </ButtonBar>

                                <TableView VBox.vgrow="ALWAYS">
                                    <columns>
                                        <TableColumn prefWidth="75.0" text="User" />
                                    </columns>
                           <columnResizePolicy>
                              <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
                           </columnResizePolicy>
                                </TableView>
                                <HBox alignment="CENTER">
                                    <Button fx:id="backButton" mnemonicParsing="false" onAction="#onBackButtonClick" style="-fx-background-color: #E67E73; -fx-text-fill: white;" text="Return to Home" />
                                </HBox>
                            </children>
                        </VBox>
                    </AnchorPane>
                </content>
            </Tab>
            <Tab text="Manage Quizzes">
                <content>
                    <AnchorPane>
                        <VBox spacing="10" AnchorPane.bottomAnchor="10" AnchorPane.leftAnchor="10" AnchorPane.rightAnchor="10" AnchorPane.topAnchor="10">
                            <children>
                                <Label style="-fx-font-size: 32; -fx-alignment: center;" text="Manage Quizzes">
                                    <VBox.margin>
                                        <Insets bottom="10" />
                                    </VBox.margin>
                                </Label>

                                <ButtonBar>
                                    <buttons>
                                        <Button alignment="CENTER" contentDisplay="CENTER" mnemonicParsing="false" style="-fx-background-color: #56D158;" text="Create Quiz" textFill="WHITE" />
                                        <Button alignment="CENTER" contentDisplay="CENTER" mnemonicParsing="false" style="-fx-background-color: #7353DB;" text="Edit Quiz" textFill="WHITE" />
                                        <Button alignment="CENTER" contentDisplay="CENTER" mnemonicParsing="false" style="-fx-background-color: #DB6353;" text="Delete Quiz" textFill="WHITE" />
                                    </buttons>
                                </ButtonBar>

                                <TableView VBox.vgrow="ALWAYS">
                                    <columns>
                                        <TableColumn prefWidth="75.0" text="Quiz" />
                                    </columns>
                           <columnResizePolicy>
                              <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
                           </columnResizePolicy>
                                </TableView>

                                <HBox alignment="CENTER">
                                    <Button fx:id="backButton3" mnemonicParsing="false" onAction="#onBackButtonClick" style="-fx-background-color: #E67E73; -fx-text-fill: white;" text="Return to Home" />
                                </HBox>
                            </children>
                        </VBox>
                    </AnchorPane>
                </content>
            </Tab>
         <Tab text="Question Management">
            <content>
               <AnchorPane>
                  <children>
                     <VBox spacing="10" AnchorPane.bottomAnchor="10" AnchorPane.leftAnchor="10" AnchorPane.rightAnchor="10" AnchorPane.topAnchor="10">
                        <children>
                           <Label style="-fx-font-size: 32; -fx-alignment: center;" text="Question Management">
                              <VBox.margin>
                                 <Insets bottom="10" />
                              </VBox.margin>
                           </Label>
                           <ButtonBar>
                              <buttons>
                                 <Button alignment="CENTER" contentDisplay="CENTER" mnemonicParsing="false" style="-fx-background-color: #56D158;" text="Add Question" textFill="WHITE" />
                                 <Button alignment="CENTER" contentDisplay="CENTER" mnemonicParsing="false" style="-fx-background-color: #7353DB;" text="Edit Question" textFill="WHITE" />
                                 <Button alignment="CENTER" contentDisplay="CENTER" mnemonicParsing="false" style="-fx-background-color: #DB6353;" text="Delete Question" textFill="WHITE" />
                              </buttons>
                           </ButtonBar>
                           <TableView VBox.vgrow="ALWAYS">
                              <columns>
                                 <TableColumn prefWidth="75.0" text="Quiz" />
                                 <TableColumn prefWidth="75.0" text="Question" />
                              </columns>
                              <columnResizePolicy>
                                 <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
                              </columnResizePolicy>
                           </TableView>
                           <HBox alignment="CENTER">
                              <children>
                                 <Button fx:id="backButton31" mnemonicParsing="false" onAction="#onBackButtonClick" style="-fx-background-color: #E67E73; -fx-text-fill: white;" text="Return to Home" />
                              </children>
                           </HBox>
                        </children>
                     </VBox>
                  </children>
               </AnchorPane>
            </content>
         </Tab>

            <Tab text="Results">
                <content>
                    <AnchorPane>
                        <VBox spacing="10" AnchorPane.bottomAnchor="10" AnchorPane.leftAnchor="10" AnchorPane.rightAnchor="10" AnchorPane.topAnchor="10">
                            <children>
                                <Label style="-fx-font-size: 32; -fx-alignment: center;" text="Results">
                                    <VBox.margin>
                                        <Insets bottom="10" />
                                    </VBox.margin>
                                </Label>

                                <ButtonBar>
                                    <buttons>
                                        <Button alignment="CENTER" contentDisplay="CENTER" mnemonicParsing="false" style="-fx-background-color: #56D158;" text="Export" textFill="WHITE" />
                                    </buttons>
                                </ButtonBar>

                                <TableView VBox.vgrow="ALWAYS">
                                    <columns>
                                        <TableColumn prefWidth="75.0" text="Quiz" />
                                        <TableColumn prefWidth="75.0" />
                                    </columns>
                                </TableView>

                                <HBox alignment="CENTER">
                                    <Button fx:id="backButton1" mnemonicParsing="false" onAction="#onBackButtonClick" style="-fx-background-color: #E67E73; -fx-text-fill: white;" text="Return to Home" />
                                </HBox>
                            </children>
                        </VBox>
                    </AnchorPane>
                </content>
            </Tab>
        </tabs>
    </TabPane>
</AnchorPane>
